{
  "name": "sav-schema",
  "version": "0.0.15",
  "description": "schema for savjs",
  "main": "dist/sav-schema.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.* test/** src/**",
    "fix": "npm run lint -- --fix",
    "start": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && npm run build && nyc ava ./test\"",
    "watch": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && nyc ava ./test/schema.js\"",
    "test": "npm run lint && npm run build && nyc ava ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savjs/sav-schema.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savjs/sav-schema/issues"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect.js": "^0.3.1",
    "nyc": "^10.0.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sav-util": "^0.0.9",
    "standard": "^8.5.0",
    "watch-cli": "^0.2.1"
  },
  "homepage": "https://github.com/savjs/sav-schema#readme",
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      [
        "resolver",
        {
          "resolveDirs": [
            "src"
          ]
        }
      ],
      "transform-object-rest-spread",
      "transform-es2015-modules-commonjs"
    ]
  }
}