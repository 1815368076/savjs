{
  "name": "savfx-router",
  "version": "1.0.23",
  "description": "another nodejs controller style router",
  "main": "dist/sav-router.cjs.js",
  "module": "dist/sav-router.es.js",
  "jsnext:main": "dist/sav-router.umd.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.config.js test/** src/**",
    "fix": "npm run lint -- --fix",
    "start": "watch -p \"+(test|src)/*.js\" -c \"npm run lint -- --fix && npm run na\"",
    "na": "nyc ava ./test",
    "test": "npm run na"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savfx/router.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savfx/router/issues"
  },
  "dependencies": {
    "sav-util": "^1.0.32"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs"
    ]
  },
  "homepage": "https://github.com/savfx/router#readme"
}
