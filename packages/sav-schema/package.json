{
  "name": "savfx-schema",
  "version": "1.0.37",
  "description": "schema for savjs",
  "main": "dist/sav-schema.cjs.js",
  "browser": "dist/sav-schema.min.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "standard rollup.* test/** src/**",
    "fix": "npm run lint -- --fix",
    "start": "watch -p \"+(test|lib)/*.js\" -c \"npm run lint -- --fix && npm run build && nyc ava ./test\"",
    "watch": "watch -p \"+(test|lib)/*.js\" -c \"npm run lint -- --fix && nyc ava ./test/schema.js\"",
    "test": "nyc ava ./test",
    "t": "nyc ava ./test/checks.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savfx/schema.git"
  },
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savfx/schema/issues"
  },
  "dependencies": {
    "sav-util": "^1.0.32"
  },
  "homepage": "https://github.com/savfx/schema#readme",
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread",
      "transform-es2015-modules-commonjs"
    ]
  }
}
